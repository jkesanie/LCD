@prefix qb4cc:          <http://data.hulib.helsinki.fi/ns/qb4cc#> .
@prefix rdf:            <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix xsd:            <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:           <http://www.w3.org/2004/02/skos/core#> .
@prefix qb:             <http://purl.org/linked-data/cube#> .
@prefix dct:        	  <http://purl.org/dc/terms/> .
@prefix foaf: 			    <http://xmlns.com/foaf/0.1/>.
@prefix lcd: 			      <http://data.hulib.helsinki.fi/ns/lcd#>.
@prefix :               <http://github.com/jkesanie/LCD/QB4CC#> .


qb4cc:WordCount
  a qb:ComponentSpecification ;
  rdfs:label "Word count" ;
  qb:measure qb4cc:wordCount ;
  qb:componentRequired "true"^^xsd:boolean;
  qb:componentAttachment qb4cc:CorpusPart;
.

qb4cc:Corpus
  a qb:ComponentSpecification ;
  rdfs:label "Corpus" ;
  qb:dimension qb4cc:corpus ;
  qb:componentRequired "true"^^xsd:boolean;
  qb:componentAttachment qb4cc:CorpusComposition;
.

qb4cc:Genre
  a qb:ComponentSpecification ;
  rdfs:label "Genre" ;
  qb:dimension qb4cc:genre ;
  qb:componentRequired "false"^^xsd:boolean;
  qb:componentAttachment qb4cc:CorpusPart;
.

qb4cc:Variety
  a qb:ComponentSpecification ;
  rdfs:label "Variety" ;
  qb:dimension qb4cc:variety ;
  qb:componentRequired "false"^^xsd:boolean;
  qb:componentAttachment qb4cc:CorpusPart;
.

qb4cc:Period
  a qb:ComponentSpecification ;
  rdfs:label "Period" ;
  qb:dimension qb4cc:timePeriod ;
  qb:componentRequired "false"^^xsd:boolean;
  qb:componentAttachment qb4cc:CorpusPart;
.
