@prefix :               <http://github.com/jkesanie/LCD/QB4CC/hc-cord1#> .

:hc-cord1-ccd
  a qb4cc:CorpusCompositionDefinition ;
  # Measure
  qb:component
    [
      qb:measure qb4cc:wordCount ;
      qb:componentRequired "true"^^xsd:boolean;
      qb:componentAttachment qb4cc:CorpusPart;
    ];
  # Dimensions
  qb:component
    [
      qb:dimension qb4cc:timePeriod ;
      qb:componentRequired "true"^^xsd:boolean;
      qb:componentAttachment qb4cc:CorpusPart;
    ],
    [
      qb:dimension qb4cc:corpus ;
      qb:componentRequired "true"^^xsd:boolean;
      qb:componentAttachment qb4cc:CorpusComposition;
    ];
.
:hc-cord1-ds
  a qb4cc:CorpusComposition ;
  dct:title "Simple corpus composition of HC using time periods.";
  dct:description "Data for the composition has been taken from
    http://www.helsinki.fi/varieng/CoRD/corpora/HelsinkiCorpus/period.html";
  qb:structure :hc-cord1-ccd ;
  qb4cc:corpus <http://h224.it.helsinki.fi:8080/varieng/data/corpus/hc> ;
.

:hc-cord1-obs1
  a qb4cc:CorpusPart ;
  qb4cc:isPartOfComposition :hc-cord1-ds ;
  qb4cc:wordCount 2190 ;
